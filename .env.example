# .env.example - copy to .env and fill values (do NOT commit .env)
# Backend (FastAPI/Flask)
FASTAPI_HOST=127.0.0.1
FASTAPI_PORT=8000
API_DEBUG=true

# External services (fill with real values in local .env)
WAZUH_HOST=127.0.0.1
WAZUH_USERNAME=admin
WAZUH_PASSWORD=
WAZUH_API_TOKEN=

MISP_API_URL=https://misp.example.com
MISP_API_KEY=

IRIS_API_URL=https://iris.example.com
IRIS_API_TOKEN=

# Frontend (Flask)
FLASK_HOST=127.0.0.1
FLASK_PORT=5000
FRONTEND_API_BASE=http://127.0.0.1:8000/api

# General
SECRET_KEY=replace_with_secure_value
LOG_LEVEL=INFO
